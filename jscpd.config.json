{
  "$schema": "https://json.schemastore.org/jscpd.json",
  "threshold": 10,
  "reporters": ["console"],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/tests/**"
  ],
  "minTokens": 50,
  "format": ["typescript", "tsx"],
  "absolute": false,
  "path": ["src"]
}
